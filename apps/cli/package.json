{
	"name": "@md-oss/cli",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"description": "CLI application",
	"license": "ISC",
	"bin": "./dist/index.js",
	"scripts": {
		"build": "pkgroll --minify --clean-dist --sourcemap  --define.process.env.NODE_ENV='\"production\"' --define.DEBUG=false",
		"clean": "git clean -xdf .turbo dist node_modules tsconfig.tsbuildinfo",
		"dev": "tsx watch --tsconfig ../../vendor/config/tsconfig.tsx.json src/index.ts",
		"start": "node ./dist/index.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@md-oss/config": "workspace:^",
		"@md-oss/utils": "workspace:^"
	},
	"devDependencies": {
		"@types/node": "^25.0.9",
		"pkgroll": "^2.21.5",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	}
}
